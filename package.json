  "scripts": {
    "start:remote": "cross-env NODE_ENV=remote nodemon -q --config ./nodemon/nodemon.config.dev.json ./src/index.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon -q --config ./nodemon/nodemon.config.dev.json ./src/index.ts",
